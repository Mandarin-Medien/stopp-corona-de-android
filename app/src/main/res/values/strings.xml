	<string name="questionnaire_guideline_precaution_number_3">Neighbours or relatives take over the care.</string>
	<string name="questionnaire_guideline_precaution_number_4">If this is not possible, contact your <b>local care services</b>.</string>
	<string name="questionnaire_guideline_precaution_number_4_phone"></string>
	<string name="questionnaire_guideline_urgent_help_headline">Do you need help urgently?</string>
	<string name="questionnaire_guideline_urgent_help_content">If you feel that you cannot manage at home, you cannot breathe or you are seriously ill, please contact the following numbers with the indication of a suspected corona infection:</string>
	<string name="questionnaire_guideline_urgent_help_emergency_label">Medical emergency service</string>
	<string name="questionnaire_guideline_urgent_help_emergency_number">116 117</string>
	<string name="questionnaire_guideline_urgent_help_ambulance_label">Ambulance</string>
	<string name="questionnaire_guideline_urgent_help_ambulance_number">112</string>
	<string name="questionnaire_guideline_advice_headline">Do you need advice or care via telephone?</string>
	<string name="questionnaire_guideline_advice_content_1">Please contact your attending physician.</string>
	<string name="questionnaire_guideline_advice_content_2">In case of psychological stress, telephone counselling provides support:</string>
	<string name="questionnaire_guideline_advice_phone_1_label">Crisis line (Telefonseelsorge)</string>
	<string name="questionnaire_guideline_advice_phone_1_number">116 123</string>
	<string name="questionnaire_guideline_advice_phone_2_label">Crisis line (Nummer gegen Kummer für Kinder und Jugendliche)</string>
	<string name="questionnaire_guideline_advice_phone_2_number">116 111</string>
	<string name="questionnaire_guideline_advice_phone_3_label"></string>
	<string name="questionnaire_guideline_advice_phone_3_number"></string>
	<string name="questionnaire_guideline_report_sick_headline">Report COVID-19</string>
	<string name="questionnaire_guideline_report_sick_content">Report a medically confirmed infection to protect your contacts.</string>
	<string name="questionnaire_guideline_report_button">Report medical confirmation</string>
	<string name="questionnaire_observe_symptoms_title">Monitor symptoms</string>
	<string name="questionnaire_observe_symptoms_sub_headline_1">Please monitor your symptoms.</string>
	<string name="questionnaire_observe_symptoms_form_filled_date">Questionnaire completed:\n%s</string>
	<string name="questionnaire_observe_symptoms_next_steps">Next steps</string>
	<string name="questionnaire_observe_symptoms_step_1">Please take your temperature again in 6 hours.</string>
	<string name="questionnaire_observe_symptoms_step_2">Then start the form again.</string>
	<string name="questionnaire_observe_symptoms_step_3">Avoid social contacts. In this way you help to avoid further infections.</string>
	<string name="questionnaire_observe_symptoms_attention_headline">Attention</string>
	<string name="questionnaire_observe_symptoms_attention_description">Please contact your attending physician by phone if you need medical attention.</string>
	<string name="questionnaire_observe_symptoms_button">Reassess symptoms</string>

