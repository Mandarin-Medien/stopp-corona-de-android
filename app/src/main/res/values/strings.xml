	<string name="revoke_sickness_report_action">Revoke infection message</string>
	<string name="revoke_sickness_success_headline">Infection message revoked</string>
	<string name="revoke_sickness_success_description_1">Thank you for updating your information and thus contributing significantly to a transparent procedure and the common good.</string>
	<string name="revoke_sickness_success_description_2">Your contacts will be anonymously notified that it was not a corona infection.</string>
	<string name="revoke_sickness_success_description_3">We wish you all the best for the future!</string>
