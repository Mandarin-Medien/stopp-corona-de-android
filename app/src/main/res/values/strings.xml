	<!-- Onboarding -->
	<string name="onboarding_headline_1">Welcome!</string>
	<string name="onboarding_copy_1_1">With the installation of the Stopp Corona app you help to</string>
	<string name="onboarding_copy_1_2">slow down</string>
	<string name="onboarding_copy_1_3">the</string>
	<string name="onboarding_copy_1_4">spread</string>
	<string name="onboarding_copy_1_5">of the novel corona virus</string>
	<string name="onboarding_headline_2">Digital handshake</string>
	<string name="onboarding_copy_2_1">Use the digital handshake to</string>
	<string name="onboarding_copy_2_2">anonymously</string>
	<string name="onboarding_copy_2_3">save your encounters. You will be notified anonymously in the app if one of your encounters falls ill.</string>
	<string name="onboarding_headline_3">Check symptoms</string>
	<string name="onboarding_copy_3_1">Use our questionnaire. Get </string>
	<string name="onboarding_copy_3_2">when required </string>
	<string name="onboarding_copy_3_3">an assessment of your health status.</string>
	<string name="onboarding_headline_4">Notification in the event of illness</string>
	<string name="onboarding_copy_4_1">Help to help others! If you should contract the corona virus yourself, your contacts will be notified</string>
	<string name="onboarding_copy_4_2">anonymously</string>
	<string name="onboarding_copy_4_3">so that they can take measures.</string>
	<string name="onboarding_headline_5">Thank you for using the app!</string>
	<string name="onboarding_copy_5_1">You\'re actively helping to protect your health and the health of others. \nYou can now record encounters with other app users.\n\nBy installing the app you agree to the</string>
	<string name="onboarding_copy_5_2">terms of use</string>
	<string name="onboarding_copy_5_3">.</string>
	<string name="onboarding_next_button">Continue</string>
	<string name="onboarding_finish_button">Finished</string>
	<string name="onboarding_headline_6">Declaration of consent</string>
	<string name="onboarding_dataprivacy_description_1">I agree that the [PLACEHOLDER] may process my personal data (unique identification numbers [IDs], and any suspicion and reporting of my COVID-19 condition [= health data]) for the purpose of rapid interruption of the corona infection chain. In addition, my public key will be shared with my intensive contacts for the purpose of issuing a warning or an all-clear notification.</string>
	<string name="onboarding_dataprivacy_approval">I consent</string>
	<string name="onboarding_dataprivacy_description_2_1">I may revoke my consent at any time, whereby revocation does not affect the lawfulness of processing up until the time that consent is revoked. \n\nFurther information can be found in our</string>
